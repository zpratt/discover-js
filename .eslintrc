{
    "env": {
        "browser": true,
        "node": true,
        "mocha": true,
        "es6": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "brace-style": [2, "1tbs"],
        "block-scoped-var": 2,
        "camelcase": 2,
        "comma-dangle": [2, "never"],
        "complexity": [2, 3],
        "curly": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "max-depth": [2, 2],
        "max-len": [0, 80, 4],
        "max-params": [2, 4],
        "max-statements": [2, 8],
        "new-cap": 0,
        "new-parens": 2,
        "no-alert": 2,
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-catch-shadow": 2,
        "no-console": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-div-regex": 2,
        "no-dupe-keys": 2,
        "no-else-return": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-empty-label": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extra-parens": 0,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-loop-func": 2,
        "no-mixed-requires": [0, false],
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-plusplus": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-shadow": 2,
        "no-sync": 2,
        "no-ternary": 0,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-underscore-dangle": 2,
        "no-unreachable": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "no-with": 2,
        "one-var": 2,
        "quote-props": 0,
        "quotes": [2, "single"],
        "radix": 0,
        "semi": 2,
        "space-after-keywords": [2, "always"],
        "space-before-blocks": [2, "always"],
        "space-before-function-paren": [
            2,
            {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "space-in-parens": [2, "never"],
        "strict": [2, "global"],
        "use-isnan": 2,
        "valid-typeof": 0,
        "vars-on-top": 2,
        "wrap-iife": 2,
        "wrap-regex": 0,
        //custom
        "no-mocha-skip-only": 2,

        "react/display-name": 2,
        "react/jsx-boolean-value": [2, "always"],
        "react/jsx-closing-bracket-location": [2, {
            "nonEmpty": "tag-aligned",
            "selfClosing": "tag-aligned"
        }],
        "react/jsx-curly-spacing": [2, "never"],
        "react/jsx-indent-props": 2,
        "react/jsx-max-props-per-line": [2, {"maximum": 1}],
        "react/jsx-no-duplicate-props": 2,
        "react/jsx-no-literals": 2,
        "react/jsx-no-undef": 2,
        "react/jsx-quotes": [2, "single", "avoid-escape"],
        "react/jsx-sort-prop-types": 2,
        "react/jsx-sort-props": 2,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/no-danger": 2,
        "react/no-did-mount-set-state": 2,
        "react/no-did-update-set-state": 2,
        "react/no-direct-mutation-state": 2,
        "react/no-multi-comp": 2,
        "react/no-unknown-property": 2,
        "react/prop-types": 2,
        "react/react-in-jsx-scope": 2,
        "react/self-closing-comp": 2,
        "react/sort-comp": 2,
        "react/wrap-multilines": 2
    }
}
